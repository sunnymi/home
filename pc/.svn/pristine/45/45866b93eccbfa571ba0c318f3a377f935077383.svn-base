.rightC .briefC .abs{display: none;z-index:10;position: absolute;top:0 left:0;width:100%;height:118px;}
.rightC .brief{width:778px;margin:0 auto;background: #eeeeee;border: 1px solid #a6a5a5;text-align: center;line-height: 30px;line-height: 58px;font-size: 14px;color:#a6a5a5;}
.rightC .brief>div.bla{height:100%;position: absolute;top:1px;left:1px;}
.rightC .brief>div.black{background: #000;position:absolute;top:0px;left:0px;filter:alpha(opacity=50); /*IE滤镜，透明度50%*/
-moz-opacity:0.5; /*Firefox私有，透明度50%*/
opacity:0.5;z-index: 9}
.rightC .brief>div>div{z-index: 0;}

