@charset "utf-8";
/* CSS Document */

html, body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, input, textarea, p, blockquote, q, th, td, caption, article, aside, figure, figcaption, footer, header, hgroup, nav, section, audio, video{
 margin: 0;
 padding: 0;
}
a,img{
 border:none;
}
img{
    display:inline-block;
}
ol, ul {
 list-style: none;
}


.container-dz {
	width: 100%;
	height: 120px;
	max-height: 150px;	
	background:#fff url() no-repeat;
	overflow: hidden;	
}

.dz-color{
	background-color: #fdf2f2;
}

.wrap990 {
	width: 990px;
	height: 120px;
	max-height: 150px;
	margin: auto;

}

.shop-log {
	width: 160px;
	height: 80px;
	padding-top: 20px;
	float: left;
	cursor: pointer;
}

.shop-text {
	height: 80px;
	line-height: 120px;
	float: left;
	padding-left: 20px;
}

.shop-name {
	font-family:"Microsoft YaHei";
	font-size:27px;
	color: #333;
}


