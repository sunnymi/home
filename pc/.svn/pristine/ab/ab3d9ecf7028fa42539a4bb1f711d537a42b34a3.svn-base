// JavaScript Document

$(document).ready(function(){
	/*第二屏下面*/
	$(window).scroll(function(){
		var h= (window.screen.height)/2; 
		var screen_h = $(this).scrollTop();
		if (screen_h > h){
			$(".QRbox .QRbox_bottom").show();
		}else{
			$(".QRbox .QRbox_bottom").hide();
		}
   	}); 
	
	/*在线或不在线*/

	
	 
});